{
  "name": "gabrycina-ws",
  "version": "1.0.0",
  "description": "gabrycina website",
  "author": "",
  "license": "GPLv3",
  "scripts": {
    "start": "node src/index.js",
    "start-dev": "nodemon index.js",
    "clean": "rimraf src/{css/*,js/*,images/*}",
    "autoprefixer": "postcss -u autoprefixer -r src/css/*",
    "scss": "node-sass --output-style compressed -o src/css src/scss",
    "imagemin": "imagemin src/images/* -o src/images",
    "serve": "browser-sync start --server --files \"src/css/*.css, src/js/*.js, **/*.html, !node_modules/**/*.html\"",
    "build:css": "run-s -scss scss autoprefixer",
    "build:js": "run-s",
    "build:images": "run-s imagemin",
    "build": "run-s build:*",
    "watch:css": "onchange \"src/scss\" -- run-s build:css",
    "watch:js": "onchange \"src/js\" -- run-s build:js",
    "watch:images": "onchange \"src/images\" -- run-s build:images",
    "watch": "run-p serve watch:*",
    "watchScss": "node-sass --watch src/scss -o src/css",
    "serve-fb": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start-fb": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "14"
  },
  "main": "src/index.js",
  "dependencies": {
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "body-parser": "^1.19.0",
    "browserify": "^17.0.0",
    "create-index": "^2.6.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "http": "0.0.1-security",
    "mailchimp-api-v3": "^1.15.0",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^0.2.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "autoprefixer": "^9.0.1",
    "babelify": "^10.0.0",
    "browser-sync": "^2.12.8",
    "imagemin-cli": "^3.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "postcss-cli": "^6.0.0",
    "rimraf": "^2.5.4",
    "stylelint": "^9.4.0",
    "uglify-es": "^3.3.10"
  },
  "homepage": "https://gabrycina.it",
  "private": true
}
